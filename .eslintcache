eps/Real-projeto-3/ironnotes-client/src/routeComponents/auth/AuthRouter.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/components/btns/BlueBtn.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/main.js",["125"],"import React, { useState, useContext } from \"react\";\n\n//Importações de componentes internos para montar troca de temas\nimport { GlobalStyle } from \"./NoteStyles/globalStyles\";\nimport { LightTheme, DarkTheme } from \"./NoteStyles/themes\";\n\n//TheProvider é um componente do Styled-components para controlar troca de temas\nimport { ThemeProvider } from \"styled-components\";\n\n//---------------------------------------------------------------------//\n\n//Component para montar a página de anotações\nimport Sidebar from \"./sidebar\";\nimport Page from \"./page\";\n//------------------------------------//\n\nfunction MainPage() {\n  //State para controlar qual tema será usado\n  const [theme, setTheme] = useState(\"light\");\n\n  return (\n    <div>\n      <ThemeProvider theme={theme === \"light\" ? LightTheme : DarkTheme}>\n        <Sidebar themes={{ theme: [theme, setTheme] }} />\n        <Page />\n        <GlobalStyle />\n      </ThemeProvider>\n    </div>\n  );\n}\n\nexport default MainPage;\n","/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/components/navbar/NavBar.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/auth/signup/SignUp.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/auth/login/Login.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/auth/profile/Profile.js",["126"],"//dependencies\nimport React, { useState, useEffect, useContext } from \"react\";\nimport { Link } from \"react-router-dom\";\n\n//images\nimport Avatar from \"../../../assets/images/LogoDark.svg\";\n\n//css\nimport \"./Profile.css\";\n\n//auth\nimport { AuthContext } from \"../../../contexts/authContext\";\n\n//axios\nimport api from \"../../../apis/pagesApi\";\n\nfunction Profile(props) {\n  const authContext = useContext(AuthContext);\n\n  const [user, setUser] = useState({\n    username: \"\",\n    cohort: \"\",\n    pagesCreated: [],\n    email: \"\",\n    avatar: \"\",\n  });\n\n  useEffect(() => {\n    async function fetchUser() {\n      try {\n        console.log(\"cheguei aqui\");\n        const response = await api.get(\"/profile\");\n        console.log(\"será que cheguei aqui?\");\n        console.log(response);\n        setUser({ ...response.data.user });\n      } catch (err) {\n        console.error(err);\n      }\n    }\n    fetchUser();\n  }, []);\n\n  // 4. \"Encaixar\" o state no JSX a ser renderizado pelo componente\n  return (\n    <div className=\"container\">\n      <div className=\"avatar-box\">\n        <img src={Avatar} style={{ width: \"150px\" }} alt=\"Avatar\" />\n\n        <button className=\"edit-button\">Edit Profile</button>\n\n        <Link to={`/delete/${user._id}`}>\n          <button className=\"delete-button\">Delete Profile</button>\n        </Link>\n      </div>\n\n      <div className=\"pages-list\">\n        <h1>{user.username}</h1>\n        <hr></hr>\n        <div className=\"infos-profile\">\n          <p>\n            <strong>E-mail: </strong>\n            {user.email}\n          </p>\n          <p>\n            <strong>Cohort: </strong>\n            {user.cohort}\n          </p>\n          <table className=\"pages\">\n            <thead>\n              <tr>\n                <th>Pages: </th>\n              </tr>\n            </thead>\n            <tbody>\n              {user.pagesCreated.map((elem) => (\n                <tr key={elem._id}>\n                  <td className=\"table-elem\">\n                    <Link to={`/pages/${elem._id}`}>{elem.title}</Link>\n                  </td>\n                </tr>\n              ))}\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Profile;\n","/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/auth/delete/DeleteUser.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/page.js",["127"],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/sidebar.js",["128"],"//Bibliotecas\nimport { Link } from \"react-router-dom\";\nimport React, { useEffect, useState, useContext } from \"react\";\nimport api from \"../../../apis/pagesApi\";\nimport { AuthContext } from \"../../../contexts/authContext\";\n//Componentes\nimport SearchPopUp from \"./PageEvents/searchpopup\";\nimport NewPage from \"./PageEvents/newpage\";\n//CSS em componentes\nimport {\n  Nav,\n  SearchBar,\n  ListItems,\n  PullNavMobile,\n  Logo,\n  ListNavTitle,\n  NavOptions,\n  Options,\n  NavRight,\n  IconRight,\n  IconListArrow,\n} from \"./NoteStyles/nav.style\";\n\n//Imagens\nimport logoWhite from \"./images/LogoLight.svg\";\nimport logoDark from \"./images/LogoDark.svg\";\nimport Dark from \"./images/nights_stay-black-18dp.svg\";\nimport Logout from \"./images/logout.svg\";\nimport Settings from \"./images/settings-black-18dp.svg\";\nimport About from \"./images/icons8-about.svg\";\nimport ArrowWhite from \"./images/keyboard_arrow_right-white-18dp.svg\";\nimport ArrowBlack from \"./images/keyboard_arrow_right-black-18dp.svg\";\nimport Sun from \"./images/wb_sunny-black-18dp.svg\";\n\nfunction Sidebar(props) {\n  const authContext = useContext(AuthContext);\n  //State para guardar todos os resultados para montar a lista na barra de navegação\n  const [list, setList] = useState([\n    {\n      _id: \"\",\n      title: \"\",\n    },\n  ]);\n\n  //useEffect para buscar dados na API\n  useEffect(() => {\n    async function Text() {\n      try {\n        const response = await api.get(\"/titles\");\n        setList([...response.data]);\n      } catch (err) {\n        console.error(err);\n      }\n    }\n    Text();\n  }, []);\n\n  //--------------------------------------------------------------------//\n\n  //State para controlar barra lateral do modo mobile\n  const [nav, setNav] = useState({\n    active: \"-100%\",\n  });\n\n  //Função para esconder ou puxar a barra de navegação\n  function Pull() {\n    if (nav.active === \"-100%\") {\n      setNav({ active: \"0%\" });\n    } else {\n      setNav({ active: \"-100%\" });\n    }\n    document.getElementById(\"Pull\").style.right = nav.active;\n  }\n\n  //------------------------------------------------------------------//\n\n  //Retirando das props o State de themas\n  const theme = props.themes.theme[0];\n  const setTheme = props.themes.theme[1];\n\n  //Fazendo a troca de Tema para dark ou light\n  const themeToggle = () => {\n    theme === \"light\" ? setTheme(\"dark\") : setTheme(\"light\");\n  };\n\n  //---------------------------------------------------------------------//\n\n  const OpenSearch = () => {\n    document.getElementById(\"SearchBarPopUp\").style.display = \"block\";\n    document.getElementById(\"SearchBarPopUpOne\").style.display = \"block\";\n  };\n\n  const OpenNewPage = () => {\n    document.getElementById(\"NewPagePopUp\").style.display = \"block\";\n    document.getElementById(\"NewPagePopUpOne\").style.display = \"block\";\n  };\n\n  return (\n    <>\n      <SearchPopUp titles={[...list]} />\n      <NewPage />\n      <NavRight>\n        <IconRight onClick={themeToggle} src={theme === \"light\" ? Dark : Sun} />\n        <IconRight src={About} />\n        <IconRight src={Settings} />\n        <IconRight src={Logout} />\n      </NavRight>\n      <PullNavMobile onClick={Pull}>Menu</PullNavMobile>\n      <Nav id=\"Pull\">\n        <Logo src={theme === \"light\" ? logoDark : logoWhite} alt=\"logo\" />\n        <SearchBar\n          placeholder=\"Search for a title here\"\n          value=\"\"\n          onClick={OpenSearch}\n        ></SearchBar>\n        <strong\n          style={{ marginTop: \"15px\", marginBottom: \"10px\", fontSize: \"20px\" }}\n        >\n          Notes\n        </strong>\n        <ListItems>\n          {list.map((list) => (\n            <Link\n              key={list._id}\n              to={`/pages/${list._id}`}\n              style={{ textDecoration: \"none\" }}\n            >\n              <ListNavTitle>\n                <IconListArrow src={theme === \"light\" ? ArrowBlack : ArrowWhite}></IconListArrow> {list.title}\n              </ListNavTitle>\n            </Link>\n          ))}\n        </ListItems>\n        <NavOptions>\n          <Options onClick={OpenNewPage}>+ New Note</Options>\n        </NavOptions>\n      </Nav>\n    </>\n  );\n}\n\nexport default Sidebar;\n","/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/globalStyles.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/themes.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/apis/pagesApi.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/components/TextInput.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/components/FileInput.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/page.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/nav.style.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/PageEvents/searchpopup.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/PageEvents/newpage.js",["129","130"],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/events.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/PageEvents/editpage.js",["131","132"],"//Bibliotecas\nimport React, { useState, useEffect, useContext } from \"react\";\nimport MDEditor from \"@uiw/react-md-editor\";\nimport api from \"../../../../apis/pagesApi\";\nimport { AuthContext } from \"../../../../contexts/authContext\";\nimport { useHistory } from \"react-router-dom\";\n\n//CSS em componentes\nimport { PopUp, ContainerPopUp } from \"../NoteStyles/events\";\nfunction Page(props) {\n  const history = useHistory();\n  const authContext = useContext(AuthContext);\n  //State para armazenar e fazer o render do conteúdo\n  const [file, setFile] = useState({\n    _id: \"\",\n    title: \"\",\n    text: \"\",\n    tags: [\"\"],\n  });\n\n  \n  //Buscando o path(Caminho) da url para retirar o Id\n  const id = history.location.pathname;\n  useEffect(() => {\n    async function Text() {\n      try {\n        console.log(id);\n        const response = await api.get(`${id}`);\n        console.log(response);\n        setFile({ ...response.data });\n        console.log(response);\n      } catch (err) {\n        console.error(err);\n      }\n    }\n    Text();\n  }, [history.location.pathname]);\n\n  function ClosePopUp() {\n    document.getElementById(\"EditPagePopUp\").style.display = \"none\";\n    document.getElementById(\"EditPagePopUpOne\").style.display = \"none\";\n  }\n\n  function handleChange(event) {\n    setFile({ ...file, [event.target.name]: event.target.value });\n    console.log(file);\n  }\n\n  function textInput(event) {\n    setFile({ ...file, text: event });\n  }\n\n  async function handleSubmit(event) {\n    event.preventDefault();\n    try {\n      const response = await api.patch(`${id}`, file);\n      window.location.reload();\n      console.log(response);\n    } catch (err) {\n      console.error(err);\n    }\n  }\n\n  async function handleDelete(event) {\n    event.preventDefault();\n    try {\n      const response = await api.delete(`${id}`);\n      window.location.reload(\"/pages/all\");\n      console.log(response);\n    } catch (err) {\n      console.error(err);\n    }\n  }\n\n\n\n  return (\n    <>\n      <PopUp id=\"EditPagePopUp\" onClick={ClosePopUp}></PopUp>\n      <ContainerPopUp id=\"EditPagePopUpOne\">\n        <form\n          style={{\n            display: \"flex\",\n            flexDirection: \"column\",\n            alignItems: \"center\",\n          }}\n        >\n          <strong>Create a new note</strong>\n          <div style={{ display: \"flex\", flexDirection: \"column\" }}>\n            <label htmlFor=\"pageTitle\">Title:</label>\n            <input\n              type=\"text\"\n              name=\"title\"\n              id=\"pageTitle\"\n              value={file.title}\n              onChange={handleChange}\n            />\n\n            <label htmlFor=\"pageTag\">Tag</label>\n            <div>\n              <input\n                type=\"text\"\n                name=\"tags\"\n                id=\"pageTags\"\n                value={file.tags}\n                onChange={handleChange}\n              />\n            </div>\n          </div>\n\n          <MDEditor value={file.text} onChange={textInput} />\n          <button onClick={handleSubmit} type=\"submit\">\n            Edit\n          </button>\n          <button onClick={handleDelete}>Delete this page</button>\n        </form>\n      </ContainerPopUp>\n    </>\n  );\n}\n\nexport default Page;\n",{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","severity":1,"message":"138","line":1,"column":27,"nodeType":"139","messageId":"140","endLine":1,"endColumn":37},{"ruleId":"137","severity":1,"message":"141","line":18,"column":9,"nodeType":"139","messageId":"140","endLine":18,"endColumn":20},{"ruleId":"137","severity":1,"message":"141","line":22,"column":9,"nodeType":"139","messageId":"140","endLine":22,"endColumn":20},{"ruleId":"137","severity":1,"message":"141","line":36,"column":9,"nodeType":"139","messageId":"140","endLine":36,"endColumn":20},{"ruleId":"137","severity