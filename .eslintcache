div>\n      <ThemeProvider theme={theme === \"light\" ? LightTheme : DarkTheme}>\n        <Sidebar themes={{ theme: [theme, setTheme] }} />\n        <Page />\n        <GlobalStyle />\n      </ThemeProvider>\n    </div>\n  );\n}\n\nexport default MainPage;\n","/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/components/TextInput.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/sidebar.js",["108"],"//Bibliotecas\nimport { Link } from \"react-router-dom\";\nimport React, { useEffect, useState, useContext } from \"react\";\nimport api from \"../../../apis/pagesApi\";\nimport { AuthContext } from \"../../../contexts/authContext\";\n//Componentes\nimport SearchPopUp from \"./PageEvents/searchpopup\";\nimport NewPage from \"./PageEvents/newpage\";\n//CSS em componentes\nimport {\n  Nav,\n  SearchBar,\n  ListItems,\n  PullNavMobile,\n  Logo,\n  ListNavTitle,\n  NavOptions,\n  Options,\n  NavRight,\n  IconRight,\n  IconListArrow,\n} from \"./NoteStyles/nav.style\";\n\n//Imagens\nimport logo from \"./images/LogoLight.svg\";\nimport Dark from \"./images/nights_stay-black-18dp.svg\";\nimport Logout from \"./images/logout.svg\";\nimport Settings from \"./images/settings-black-18dp.svg\";\nimport About from \"./images/icons8-about.svg\";\nimport Arrow from \"./images/keyboard_arrow_right-white-18dp.svg\";\nimport Sun from \"./images/wb_sunny-black-18dp.svg\";\n\nfunction Sidebar(props) {\n  const authContext = useContext(AuthContext);\n  //State para guardar todos os resultados para montar a lista na barra de navegação\n  const [list, setList] = useState([\n    {\n      _id: \"\",\n      title: \"\",\n    },\n  ]);\n\n  //useEffect para buscar dados na API\n  useEffect(() => {\n    async function Text() {\n      try {\n        const response = await api.get(\"/pages/titles\");\n        setList([...response.data]);\n      } catch (err) {\n        console.error(err);\n      }\n    }\n    Text();\n  }, []);\n\n  //--------------------------------------------------------------------//\n\n  //State para controlar barra lateral do modo mobile\n  const [nav, setNav] = useState({\n    active: \"-100%\",\n  });\n\n  //Função para esconder ou puxar a barra de navegação\n  function Pull() {\n    if (nav.active === \"-100%\") {\n      setNav({ active: \"0%\" });\n    } else {\n      setNav({ active: \"-100%\" });\n    }\n    document.getElementById(\"Pull\").style.right = nav.active;\n  }\n\n  //------------------------------------------------------------------//\n\n  //Retirando das props o State de themas\n  const theme = props.themes.theme[0];\n  const setTheme = props.themes.theme[1];\n\n  //Fazendo a troca de Tema para dark ou light\n  const themeToggle = () => {\n    theme === \"light\" ? setTheme(\"dark\") : setTheme(\"light\");\n  };\n\n  //---------------------------------------------------------------------//\n\n  const OpenSearch = () => {\n    document.getElementById(\"SearchBarPopUp\").style.display = \"block\";\n    document.getElementById(\"SearchBarPopUpOne\").style.display = \"block\";\n  };\n\n  const OpenNewPage = () => {\n    document.getElementById(\"NewPagePopUp\").style.display = \"block\";\n    document.getElementById(\"NewPagePopUpOne\").style.display = \"block\";\n  };\n\n  return (\n    <>\n      <SearchPopUp titles={[...list]} />\n      <NewPage />\n      <NavRight>\n        <IconRight onClick={themeToggle} src={theme === \"light\" ? Dark : Sun} />\n        <IconRight src={About} />\n        <IconRight src={Settings} />\n        <IconRight src={Logout} />\n      </NavRight>\n      <PullNavMobile onClick={Pull}>Menu</PullNavMobile>\n      <Nav id=\"Pull\">\n        <Logo src={logo} alt=\"logo\" />\n        <SearchBar\n          placeholder=\"Search for a title here\"\n          value=\"\"\n          onClick={OpenSearch}\n        ></SearchBar>\n        <strong\n          style={{ marginTop: \"15px\", marginBottom: \"10px\", fontSize: \"20px\" }}\n        >\n          Notes\n        </strong>\n        <ListItems>\n          {list.map((list) => (\n            <Link\n              key={list._id}\n              to={`/page/${list._id}`}\n              style={{ textDecoration: \"none\" }}\n            >\n              <ListNavTitle>\n                <IconListArrow src={Arrow}></IconListArrow> {list.title}\n              </ListNavTitle>\n            </Link>\n          ))}\n        </ListItems>\n        <NavOptions>\n          <Options onClick={OpenNewPage}>+ New Note</Options>\n        </NavOptions>\n      </Nav>\n    </>\n  );\n}\n\nexport default Sidebar;\n","/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/page.js",["109"],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/apis/pagesApi.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/globalStyles.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/themes.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/nav.style.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/page.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/PageEvents/searchpopup.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/PageEvents/newpage.js",["110"],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/events.js",[],{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","replacedBy":"114"},{"ruleId":"115","severity":1,"message":"116","line":1,"column":27,"nodeType":"117","messageId":"118","endLine":1,"endColumn":37},{"ruleId":"115","severity":1,"message":"119","line":34,"column":9,"nodeType":"117","messageId":"118","endLine":34,"end