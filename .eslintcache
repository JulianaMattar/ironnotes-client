ht\");\n\n  return (\n    <div>\n      <ThemeProvider theme={theme === \"light\" ? LightTheme : DarkTheme}>\n        <Sidebar themes={{ theme: [theme, setTheme] }} />\n        <Page />\n        <GlobalStyle />\n      </ThemeProvider>\n    </div>\n  );\n}\n\nexport default MainPage;\n","/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/components/TextInput.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/sidebar.js",["108"],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/page.js",["109"],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/apis/pagesApi.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/globalStyles.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/themes.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/nav.style.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/page.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/PageEvents/searchpopup.js",[],"/home/leonardouezu/Reps/Real-projeto-3/ironnotes-client/src/routeComponents/pages/NoteComponents/PageEvents/newpage.js",["110"],"import React, { useState, useContext } from \"react\";\nimport MDEditor from \"@uiw/react-md-editor\";\n\nimport { PopUp, ContainerPopUp } from \"../NoteStyles/events\";\nimport api from \"../../../../apis/pagesApi\";\nimport { AuthContext } from \"../../../../contexts/authContext\";\n\nfunction NewPage(props) {\n  const authContext = useContext(AuthContext);\n  const [page, setPage] = useState({\n    title: \"\",\n    tags: \"\",\n    text: \"\",\n  });\n\n  function handleChange(event) {\n    setPage({ ...page, [event.target.name]: event.target.