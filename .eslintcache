ts/project3-app/src/routeComponents/auth/AuthRouter.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/pages/PagesRouter.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/pages/NoteComponents/quotes/Quotes.js",["143","144"],"import { useEffect, useState, useContext, React } from \"react\";\nimport api from \"../../../../apis/pagesApi\";\nimport { AuthContext } from \"../../../../contexts/authContext\";\n\nimport { TitleH3, FormButton } from \"../NoteStyles/page\";\nimport { PopUp, ContainerPopUp } from \"../NoteStyles/events\";\nimport \"./Quote.css\";\n\nimport NewQuote from \"./AddNewQuote\";\n\nfunction QuotesPopUp(props) {\n  function ClosePopUp() {\n    document.getElementById(\"QuotesPopUp\").style.display = \"none\";\n    document.getElementById(\"QuotesPopUpOne\").style.display = \"none\";\n  }\n\n  const authContext = useContext(AuthContext);\n\n  //State para puxar os quotes\n  const [quote, setQuote] = useState({\n    said_by: \"\",\n    quote: \"\",\n  });\n\n  const [random, setRandom] = useState(false);\n\n  //useEffect para buscar dados na API\n  useEffect(() => {\n    async function fetchQuotes() {\n      try {\n        const response = await api.get(\"/quote\");\n        console.log(response);\n        setQuote({ ...response.data });\n      } catch (err) {\n        console.error(err);\n      }\n    }\n    fetchQuotes();\n  }, [random]);\n\n  function handleClick() {\n    setRandom(!random);\n  }\n\n  function showForm() {\n    document.getElementById(\"modal\").style.display = \"none\";\n    document.getElementById(\"addForm\").style.display = \"block\";\n  }\n\n  return (\n    <>\n      <PopUp id=\"QuotesPopUp\" onClick={ClosePopUp}></PopUp>\n      <ContainerPopUp id=\"QuotesPopUpOne\">\n        <div id=\"modal\">\n          <div>\n            <h3>\n              <i>\"{quote.quote}\"</i>\n            </h3>\n            <p id=\"author\">Said By: {quote.said_by}</p>\n          </div>\n          <div id=\"buttons\">\n          <FormButton onClick={handleClick}>Random Quote</FormButton>\n          <FormButton onClick={showForm}>Add a Quote</FormButton>\n          </div>\n        </div>\n        <NewQuote />\n      </ContainerPopUp>\n    </>\n  );\n}\n\nexport default QuotesPopUp;\n","/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/components/btns/BlueBtn.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/pages/NoteComponents/Page.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/pages/NoteComponents/HowTo.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/auth/signup/SignUp.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/components/navbar/NavBar.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/auth/profile/Profile.js",["145"],"//dependencies\nimport React, { useState, useEffect, useContext } from \"react\";\nimport { Link } from \"react-router-dom\";\n\n//images\nimport Avatar from \"../../../assets/images/LogoDark.svg\";\n\n//css\nimport \"./Profile.css\";\n\n//auth\nimport { AuthContext } from \"../../../contexts/authContext\";\n\n//axios\nimport api from \"../../../apis/pagesApi\";\n\nfunction Profile(props) {\n  useContext(AuthContext);\n\n  const [user, setUser] = useState({\n    username: \"\",\n    cohort: \"\",\n    pagesCreated: [],\n    email: \"\",\n    avatar: \"\",\n  });\n\n  useEffect(() => {\n    async function fetchUser() {\n      try {\n        const response = await api.get(\"/profile\");\n        setUser({ ...response.data.user });\n      } catch (err) {\n        console.error(err);\n      }\n    }\n    fetchUser();\n  }, []);\n\n  // 4. \"Encaixar\" o state no JSX a ser renderizado pelo componente\n  return (\n    <div className=\"container\">\n      <div className=\"avatar-box\">\n        <img src={user.avatar} style={{ width: \"150px\" }} alt=\"Avatar\" />\n        <Link to={`/auth/profile/edit`}>\n          <button className=\"edit-button\">Edit Profile</button>\n        </Link>\n\n        <Link to={`/auth/delete/${user._id}`}>\n          <button className=\"delete-button\">Delete Profile</button>\n        </Link>\n      </div>\n\n      <div className=\"pages-list\">\n        <h1>{user.username}</h1>\n        <hr></hr>\n        <div className=\"infos-profile\">\n          <p>\n            <strong>E-mail: </strong>\n            {user.email}\n          </p>\n          <p>\n            <strong>Cohort: </strong>\n            {user.cohort}\n          </p>\n          <table className=\"pages\">\n            <thead>\n              <tr>\n                <th>Pages: </th>\n              </tr>\n            </thead>\n            <tbody>\n              {user.pagesCreated.map((elem) => (\n                <tr key={elem._id}>\n                  <td className=\"table-elem\">\n                    <Link to={`/pages/${elem._id}`}>{elem.title}</Link>\n                  </td>\n                </tr>\n              ))}\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Profile;\n","/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/auth/delete/DeleteUser.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/auth/login/Login.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/auth/about/About.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/auth/profile/EditProfile.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/pages/NoteComponents/NoteStyles/themes.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/pages/NoteComponents/NoteStyles/globalStyles.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/pages/NoteComponents/NoteStyles/events.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/pages/NoteComponents/NoteStyles/page.js",[],"/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/pages/NoteComponents/Sidebar.js",["146"],"//Bibliotecas\nimport { Link } from \"react-router-dom\";\nimport React, { useEffect, useState, useContext } from \"react\";\nimport api from \"../../../apis/pagesApi\";\nimport { AuthContext } from \"../../../contexts/authContext\";\n\n//Componentes\nimport NewPage from \"./PageEvents/NewPage\";\nimport QuotesPopUp from \"./quotes/Quotes\";\n\n//CSS em componentes\nimport {\n  Nav,\n  SearchBar,\n  ListItems,\n  PullNavMobile,\n  Logo,\n  ListNavTitle,\n  NavOptions,\n  Options,\n  NavRight,\n  IconRight,\n  IconListArrow,\n} from \"./NoteStyles/nav.style\";\n\n//Imagens\nimport logoWhite from \"../../../assets/images/LogoLight.svg\";\nimport logoDark from \"../../../assets/images/LogoDark.svg\";\nimport Dark from \"../../../assets/icons/nights_stay-black-18dp.svg\";\nimport LogoutBlack from \"../../../assets/icons/power_black.svg\";\nimport LogoutWhite from \"../../../assets/icons/power_white.svg\";\nimport SettingsBlack from \"../../../assets/icons/settings_black.svg\";\nimport SettingsWhite from \"../../../assets/icons/settings_white.svg\";\nimport AboutBlack from \"../../../assets/icons/about_black.svg\";\nimport AboutWhite from \"../../../assets/icons/about_white.svg\";\nimport ArrowWhite from \"../../../assets/icons/keyboard_arrow_right-white-18dp.svg\";\nimport ArrowBlack from \"../../../assets/icons/keyboard_arrow_right-black-18dp.svg\";\nimport Sun from \"../../../assets/icons/wb_sunny-black-18dp.svg\";\nimport EmojiBlack from \"../../../assets/icons/tongue_black.svg\";\nimport EmojiWhite from \"../../../assets/icons/tongue_white.svg\";\n\nfunction Sidebar(props) {\n  const authContext = useContext(AuthContext);\n  //State para guardar todos os resultados para montar a lista na barra de navegação\n  const [list, setList] = useState([\n    {\n      _id: \"\",\n      title: \"\",\n    },\n  ]);\n\n  //useEffect para buscar dados na API\n  useEffect(() => {\n    async function Text() {\n      try {\n        const response = await api.get(\"/titles\");\n        setList([...response.data]);\n      } catch (err) {\n        console.error(err);\n      }\n    }\n    Text();\n  }, []);\n\n  //--------------------------------------------------------------------//\n\n  //State para controlar barra lateral do modo mobile\n  const [nav, setNav] = useState({\n    active: \"-100%\",\n  });\n\n  //Função para esconder ou puxar a barra de navegação\n  function Pull() {\n    if (nav.active === \"-100%\") {\n      setNav({ active: \"0%\" });\n    } else {\n      setNav({ active: \"-100%\" });\n    }\n    document.getElementById(\"Pull\").style.right = nav.active;\n  }\n\n  //Function to logout\n  function handleLogoutClick() {\n    localStorage.clear();\n    authContext.setLoggedInUser({ token: \"\", user: {} });\n    props.history.push(\"/\");\n  }\n\n  //------------------------------------------------------------------//\n\n  //Retirando das props o State de themas\n  const theme = props.themes.theme[0];\n  const setTheme = props.themes.theme[1];\n\n  //Fazendo a troca de Tema para dark ou light\n  const themeToggle = () => {\n    theme === \"light\" ? setTheme(\"dark\") : setTheme(\"light\");\n  };\n\n  //------------------------------------------------------------------//\n\n  const toggleQuotes = () => {\n    document.getElementById(\"QuotesPopUp\").style.display = \"block\";\n    document.getElementById(\"QuotesPopUpOne\").style.display = \"block\";\n  };\n\n  return (\n    <>\n      <QuotesPopUp />\n      <NavRight>\n        <IconRight onClick={themeToggle} src={theme === \"light\" ? Dark : Sun} />\n        <IconRight\n          onClick={toggleQuotes}\n          src={theme === \"light\" ? EmojiBlack : EmojiWhite}\n        />\n        <Link to=\"/auth/about\">\n          <IconRight src={theme === \"light\" ? AboutBlack : AboutWhite} />\n        </Link>\n        <Link to=\"/auth/profile\">\n          <IconRight src={theme === \"light\" ? SettingsBlack : SettingsWhite} />\n        </Link>\n        <IconRight\n          onClick={handleLogoutClick}\n          src={theme === \"light\" ? LogoutBlack : LogoutWhite}\n        />\n      </NavRight>\n      <PullNavMobile onClick={Pull}>Menu</PullNavMobile>\n      <Nav id=\"Pull\">\n        <Link to=\"/\">\n          <Logo src={theme === \"light\" ? logoDark : logoWhite} alt=\"logo\" />\n        </Link>\n        <Link\n          to=\"/pages/search\"\n          style={{ width: \"100%\", display: \"flex\", justifyContent: \"center\" }}\n        >\n          <SearchBar placeholder=\"Search here\"></SearchBar>\n        </Link>\n\n        <ListItems>\n          {list.map((list) => (\n            <Link\n              key={list._id}\n              to={`/pages/${list._id}`}\n              style={{ textDecoration: \"none\" }}\n            >\n              <ListNavTitle>\n                <IconListArrow\n                  src={theme === \"light\" ? ArrowBlack : ArrowWhite}\n                ></IconListArrow>{\" \"}\n                {list.title}\n              </ListNavTitle>\n            </Link>\n          ))}\n        </ListItems>\n        <NavOptions>\n          <Options>\n            <Link to=\"/pages/newpage\">+ New Note</Link>\n          </Options>\n        </NavOptions>\n      </Nav>\n    </>\n  );\n}\n\nexport default Sidebar;\n","/Users/tassiaaccioly/Documents/Programação/ironhack/projects/project3-app/src/routeComponents/pages/NoteComponents/PageEvents/EditPage.js",[],"/Users/tassiaaccioly/Documents/Programaça