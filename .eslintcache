hemeProvider theme={theme === \"light\" ? LightTheme : DarkTheme}>\n        <Sidebar themes={{ theme: [theme, setTheme] }} />\n        <Switch>\n          <Route\n            exact\n            path={`${props.match.path}`}\n            render={(routeProps) => {\n              return <HowTo themes={{ theme: [theme, setTheme] }} />;\n            }}\n          />\n          <Route\n            exact\n            path={`${props.match.path}/search`}\n            render={(routeProps) => {\n              return <Search themes={{ theme: [theme, setTheme] }} />;\n            }}\n          />\n          <Route path={`${props.match.path}/:id`} component={Page} />\n          <GlobalStyle />\n        </Switch>\n      </ThemeProvider>\n    </React.Fragment>\n  );\n}\n\nexport default PagesRouter;\n","/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/components/btns/BlueBtn.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/auth/signup/SignUp.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/auth/profile/Profile.js",["141"],"//dependencies\nimport React, { useState, useEffect, useContext } from \"react\";\nimport { Link } from \"react-router-dom\";\n\n//images\nimport Avatar from \"../../../assets/images/LogoDark.svg\";\n\n//css\nimport \"./Profile.css\";\n\n//auth\nimport { AuthContext } from \"../../../contexts/authContext\";\n\n//axios\nimport api from \"../../../apis/pagesApi\";\n\nfunction Profile(props) {\n  const authContext = useContext(AuthContext);\n\n  const [user, setUser] = useState({\n    username: \"\",\n    cohort: \"\",\n    pagesCreated: [],\n    email: \"\",\n    avatar: \"\",\n  });\n\n  useEffect(() => {\n    async function fetchUser() {\n      try {\n        console.log(\"cheguei aqui\");\n        const response = await api.get(\"/profile\");\n        console.log(\"será que cheguei aqui?\");\n        console.log(response);\n        setUser({ ...response.data.user });\n      } catch (err) {\n        console.error(err);\n      }\n    }\n    fetchUser();\n  }, []);\n\n  // 4. \"Encaixar\" o state no JSX a ser renderizado pelo componente\n  return (\n    <div className=\"container\">\n      <div className=\"avatar-box\">\n        <img src={Avatar} style={{ width: \"150px\" }} alt=\"Avatar\" />\n\n        <button className=\"edit-button\">Edit Profile</button>\n\n        <Link to={`/delete/${user._id}`}>\n          <button className=\"delete-button\">Delete Profile</button>\n        </Link>\n      </div>\n\n      <div className=\"pages-list\">\n        <h1>{user.username}</h1>\n        <hr></hr>\n        <div className=\"infos-profile\">\n          <p>\n            <strong>E-mail: </strong>\n            {user.email}\n          </p>\n          <p>\n            <strong>Cohort: </strong>\n            {user.cohort}\n          </p>\n          <table className=\"pages\">\n            <thead>\n              <tr>\n                <th>Pages: </th>\n              </tr>\n            </thead>\n            <tbody>\n              {user.pagesCreated.map((elem) => (\n                <tr key={elem._id}>\n                  <td className=\"table-elem\">\n                    <Link to={`/pages/${elem._id}`}>{elem.title}</Link>\n                  </td>\n                </tr>\n              ))}\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Profile;\n","/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/auth/login/Login.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/components/navbar/NavBar.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/auth/delete/DeleteUser.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/globalStyles.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/HowTo.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/themes.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/PageEvents/searchpage/SearchPage.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/components/TextInput.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/components/FileInput.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/apis/pagesApi.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/page.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/events.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/components/CheckInput.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/main.js",["142"],"import React, { useState, useContext } from \"react\";\n\n//Importações de componentes internos para montar troca de temas\nimport { GlobalStyle } from \"./NoteStyles/globalStyles\";\nimport { LightTheme, DarkTheme } from \"./NoteStyles/themes\";\n\n//TheProvider é um componente do Styled-components para controlar troca de temas\nimport { ThemeProvider } from \"styled-components\";\n\n//---------------------------------------------------------------------//\n\n//Component para montar a página de anotações\nimport Sidebar from \"./sidebar\";\nimport Page from \"./page\";\n\n//------------------------------------//\n\nfunction MainPage() {\n  //State para controlar qual tema será usado\n  const [theme, setTheme] = useState(\"light\");\n\n  return (\n    <div>\n      <ThemeProvider theme={theme === \"light\" ? LightTheme : DarkTheme}>\n        <Sidebar themes={{ theme: [theme, setTheme] }} />\n        <Page />\n        <GlobalStyle />\n      </ThemeProvider>\n    </div>\n  );\n}\n\nexport default MainPage;\n","/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/page.js",["143"],"//Bibliotecas\nimport React, { useState, useEffect, useContext } from \"react\";\nimport MDEditor from \"@uiw/react-md-editor\";\nimport api from \"../../../apis/pagesApi\";\nimport { AuthContext } from \"../../../contexts/authContext\";\n\n//CSS em componentes\nimport {\n  Container,\n  Fix,\n  Button,\n  Title,\n  Tag,\n  TagQueue,\n  FixHTML,\n} from \"./NoteStyles/page\";\nimport EditPage from \"./PageEvents/editpage\";\n\nfunction Page(props) {\n  const authContext = useContext(AuthContext);\n  //State para armazenar e fazer o render do conteúdo\n  const [file, setFile] = useState({\n    _id: \"\",\n    title: \"\",\n    text: \"\",\n    tags: [\"\"],\n  });\n\n  //Buscando o path(Caminho) da url para retirar o Id\n\n  useEffect(() => {\n    async function Text() {\n      try {\n        const { id } = props.match.params;\n\n        console.log(id);\n        const response = await api.get(`/pages/${id}`);\n        console.log(response);\n        setFile({ ...response.data });\n        console.log(response);\n      } catch (err) {\n        console.error(err);\n      }\n    }\n    Text();\n  }, [props]);\n\n  const OpenSearch = () => {\n    document.getElementById(\"EditPagePopUp\").style.display = \"block\";\n    document.getElementById(\"EditPagePopUpOne\").style.display = \"block\";\n  };\n\n  return (\n    <>\n      <EditPage />\n      <FixHTML></FixHTML>\n      <Container>\n        <Fix>\n          <header>\n            <Title>{file.title}</Title>\n            <Tag>\n              {file.tags.map((file, i) => {\n                return <TagQueue key={i}>{file}</TagQueue>;\n              })}\n            </Tag>\n          </header>\n          <MDEditor.Markdown source={file.text} />\n          <div\n            style={{ width: \"100%\", display: \"flex\", justifyContent: \"center\" }}\n          >\n            <Button onClick={OpenSearch}>Edit Note</Button>\n          </div>\n        </Fix>\n      </Container>\n    </>\n  );\n}\n\nexport default Page;\n","/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/PageEvents/editpage.js",["144","145"],"//Bibliotecas\nimport React, { useState, useEffect, useContext } from \"react\";\nimport MDEditor from \"@uiw/react-md-editor\";\nimport api from \"../../../../apis/pagesApi\";\nimport { AuthContext } from \"../../../../contexts/authContext\";\nimport { useHistory } from \"react-router-dom\";\n\n//CSS em componentes\nimport {\n  PopUp,\n  ContainerPopUp,\n  FormPopUp,\n  InputForm,\n} from \"../NoteStyles/events\";\nimport { Button } from \"../NoteStyles/page\";\nfunction Page(props) {\n  const history = useHistory();\n  const authContext = useContext(AuthContext);\n  //State para armazenar e fazer o render do conteúdo\n  const [file, setFile] = useState({\n    _id: \"\",\n    title: \"\",\n    text: \"\",\n    tags: [\"\"],\n  });\n\n  //Buscando o path(Caminho) da url para retirar o Id\n  const id = history.location.pathname;\n  useEffect(() => {\n    async function Text() {\n      try {\n        console.log(id);\n        const response = await api.get(`${id}`);\n        console.log(response);\n        setFile({ ...response.data });\n        console.log(response);\n      } catch (err) {\n        console.error(err);\n      }\n    }\n    Text();\n  }, [history.location.pathname]);\n\n  function ClosePopUp() {\n    document.getElementById(\"EditPagePopUp\").style.display = \"none\";\n    document.getElementById(\"EditPagePopUpOne\").style.display = \"none\";\n  }\n\n  function handleChange(event) {\n    setFile({ ...file, [event.target.name]: event.target.value });\n    console.log(file);\n  }\n\n  function textInput(event) {\n    setFile({ ...file, text: event });\n  }\n\n  async function handleSubmit(event) {\n    event.preventDefault();\n    try {\n      const response = await api.patch(`${id}`, file);\n      window.location.reload();\n      console.log(response);\n    } catch (err) {\n      console.error(err);\n    }\n  }\n\n  async function handleDelete(event) {\n    event.preventDefault();\n    try {\n      const response = await api.delete(`${id}`);\n      async function reloadPage() {\n        const response = await api.get(\"/titles\");\n        const { _id } = response.data[response.data.length - 1];\n        window.location.replace(`/pages/${_id}`);\n      }\n      reloadPage();\n      console.log(response);\n    } catch (err) {\n      console.error(err);\n    }\n  }\n\n  return (\n    <>\n      <PopUp id=\"EditPagePopUp\" onClick={ClosePopUp}></PopUp>\n      <ContainerPopUp id=\"EditPagePopUpOne\">\n        <FormPopUp>\n          <strong>Edit this note</strong>\n          <div\n            style={{\n              display: \"flex\",\n              flexDirection: \"column\",\n              width: \"100%\",\n              marginBottom: \"2%\",\n            }}\n          >\n            <label htmlFor=\"pageTitle\">Title:</label>\n            <InputForm\n              type=\"text\"\n              name=\"title\"\n              id=\"pageTitle\"\n              value={file.title}\n              onChange={handleChange}\n            />\n\n            <label htmlFor=\"pageTags\">Tags: </label>\n            <div>\n              <InputForm\n                type=\"text\"\n                name=\"tags\"\n                id=\"pageTags\"\n                value={file.tags}\n                onChange={handleChange}\n              />\n            </div>\n          </div>\n\n          <MDEditor\n            value={file.text}\n            onChange={textInput}\n            height={350}\n            width={400}\n          />\n          <div\n            style={{\n              width: \"30%\",\n              display: \"flex\",\n              justifyContent: \"center\",\n            }}\n          >\n            <Button onClick={handleSubmit} 