ags: [\"\"],\n  });\n\n  //Buscando o path(Caminho) da url para retirar o Id\n\n  useEffect(() => {\n    async function Text() {\n      try {\n        const id = history.location.pathname;\n\n\nconsole.log(id)\n        const response = await api.get(`${id}`);\n        console.log(response)\n        setFile({ ...response.data });\n        console.log(response)\n      } catch (err) {\n        console.error(err);\n      }\n    }\n    Text();\n  }, [history.location.pathname]);\n\n  return (\n    <>\n      <Container>\n        <Fix>\n          <header>\n            <Title>{file.title}</Title>\n            <Tag>\n              {file.tags.map((file, i) => {\n                return <TagQueue key={i}>{file}</TagQueue>;\n              })}\n            </Tag>\n          </header>\n          {<MDEditor.Markdown source={file.text} />}\n          <Button>Edit this note</Button>\n        </Fix>\n      </Container>\n    </>\n  );\n}\n\nexport default Page;\n","/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/components/TextInput.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/nav.style.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/NoteStyles/page.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/PageEvents/searchpopup.js",[],"/Users/Juliana/Desktop/IronNotes - FRONTEND/ironnotes-client/src/routeComponents/pages/NoteComponents/PageEvents/newpage.js",["128"],"import React, { useState, useContext } from \"react\";\nimport MDEditor from \"@uiw/react-md-editor\";\n\nimport { PopUp, ContainerPopUp } from \"../NoteStyles/events\";\nimport api from \"../../../../apis/pagesApi\";\nimport { AuthContext } from \"../../../../contexts/authContext\";\n\nfunction NewPage(props) {\n  const authContext = useContext(AuthContext);\n  const [page, setPage] = useState({\n    title: \"\",\n   